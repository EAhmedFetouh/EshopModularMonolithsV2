{
  "ConnectionStrings": {
    "Database": "Server=localhost;port=5432;Database=EshopDb;User Id=postgres;Password=postgres",
    "Redis": "localhost:6379"
  },
  "MessageBroker": {
    "Host": "amqp://localhost:5672",
    "UserName": "guest",
    "Password": "guest"
  },

  "keycloak": {
    "realm": "myrealm",
    "auth-server-url": "http://localhost:9090/",
    "resource": "myclient",
    "ssl-required": "none",
    "confidential-port": 0,
    "verify-token-audience": false,
    "credentials": {
      "secret": ""
    }
  },

  //"Logging": {
  //  "LogLevel": {
  //    "Default": "Information",
  //    "Microsoft.AspNetCore": "Warning"
  //  }
  //},
    "Serilog": {
      "Using": [
        "Serilog.Sinks.Console",
        "Serilog.Sinks.seq"
      ],
      "MinimumLevel": {
        "Default": "Information",
        "Override": {
          "Microsoft": "Information",
          "System": "Warning"
        }
      },
      "WriteTo": [
        {
          "Name": "Console"
        },
        {
          "Name": "Seq",
          "Args": {
            "serverUrl": "http://localhost:5341"
          }
        }
      ],
      "Enrich": [
        "FromLogContext",
        "WithMachineName",
        "WithProcessId",
        "WithThreadId"
      ],
      "Properties": {
        "Application": "EShop ASP.NET Core App",
        "Environment": "Development"
      }
    },
  "AllowedHosts": "*"
}
